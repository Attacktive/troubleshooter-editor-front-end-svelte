{
	"name": "troubleshooter-editor",
	"version": "2.0.3",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000 --host",
		"serve": "vite dev --port 3000 --host --mode production",
		"build": "vite build",
		"build:production": "vite build --mode production",
		"preview": "vite preview --port 3000 --host",
