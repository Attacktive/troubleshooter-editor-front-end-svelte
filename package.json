		"@types/node": "^24.10.0",
		"@typescript-eslint/eslint-plugin": "^8.46.3",
		"@typescript-eslint/parser": "^8.46.3",
		"eslint": "^9.39.1",
		"eslint-plugin-svelte": "^3.13.0",
		"flowbite": "^3.1.2",
